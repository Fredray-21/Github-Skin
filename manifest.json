{
  "name": "Github-skin",
  "description": "Skin for Github",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "popup.html",
        "popup.js",
        "modeles/*",
        "assets/*"
      ],
      "matches": [
        "https://github.com/*"
      ]
    }
  ]
}